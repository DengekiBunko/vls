{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "listen": null,
      "port": $PORT,
      "protocol": "vless",
      "settings": {
        "decryption": "none",
        "clients": [
          {
            "email": "idx-xhttp",
            "flow": "",
            "id": "$UUID"
          }
        ]
      },
      "streamSettings": {
        "network": "xhttp",
        "xhttpSettings": {
          "host": "",
          "mode": "auto",
          "path": "/"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom"
    }
  ]
}
